#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b;
    float c,d;
    scanf("%d %d \n",&a, &b);
    scanf("%f %f",&c,&d);
    int sum=a+b;
    int diff=a-b;
    float Sum1=c+d;
    float diff1=c-d;
    printf("%d ",sum);
    printf("%d\n",diff);
    printf("%0.1f ",Sum1);
    printf("%0.1f",diff1);
    return 0;
}
